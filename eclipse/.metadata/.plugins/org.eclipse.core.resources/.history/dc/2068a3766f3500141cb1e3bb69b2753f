    public class mysword_1 extends ItemSword {

    protected mysword_1(ToolMaterial p_i45347_1_) {
            super(p_i45347_1_);
            
        }
        
       @SideOnly(Side.CLIENT)
        public void registerIcons(IIconRegister iconRegister) {
            this.itemIcon = iconRegister.registerIcon(mymod.MODID + ":" + "mypickaxe_1");
        }
    
    }
